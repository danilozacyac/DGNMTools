<telerik:RadWindow
    x:Class="DGNMTools.TipoSociedad.GeneraTipoSociedad"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    Width="450"
    Height="300"
    Header="GeneraTipoSociedad"
    Loaded="RadWindow_Loaded">
    <Grid>
        <Button
            x:Name="BtnSinEspacios"
            Width="75"
            Margin="83,45,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="BtnSinEspacios_Click"
            Content="Sin Espacios"
            ToolTip="Elimina los espacios presentes en la denominación y hace la asociación verificando si la paalabra que se genera fibnaliza con alguna de las síglas del catálogo" />
        <Button
            x:Name="BtnAfterComa"
            Width="128"
            Margin="97,92,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="BtnAfterComa_Click"
            Content="Después de la coma"
            ToolTip="Verifica si la información que esta después de la última &quot;,&quot; coincide con las siglas de los tipos de denominaciones" />

    </Grid>
</telerik:RadWindow>